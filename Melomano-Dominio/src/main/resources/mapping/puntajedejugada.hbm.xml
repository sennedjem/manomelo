<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="melomano.dominio.PuntajeDeJugada" table="Puntaje" >
        <id name="idPuntaje" type="java.lang.Integer">
            <column name="ID_PUNTAJE" />
            <generator class="identity" />
        </id>
        
        <property name="nombre" column="Nombre" type="string" not-null="true" />
        <property name="puntaje" column="Puntaje" not-null="true" />
		<property name="genero" column="Genero" type="string" not-null="true" />
        
 </class>
</hibernate-mapping>